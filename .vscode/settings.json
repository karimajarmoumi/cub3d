{
	"terminal.integrated.env.linux": {
        "LD_PRELOAD": null
    },

	"files.associations": {
		"*.embeddedhtml": "html",
		"stdlib.h": "c"
	},

    "terminal.integrated.profiles.linux": {
        "zsh": {
            "path": "/usr/bin/flatpak-spawn",
            "args": ["--host", "--env=TERM=xterm-256color", "zsh"]
        },
        "bash": {
            "path": "/bin/bash",
            "icon": "terminal-bash"
        },
        "fish": {
            "path": "/bin/fish"
        },
        "tmux": {
            "path": "/bin/tmux",
            "icon": "terminal-tmux"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        }
    },
    "terminal.integrated.defaultProfile.linux": "zsh",
}